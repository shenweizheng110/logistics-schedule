{
    "presets": [
        "@babel/react",
        "@babel/typescript",
        [
            "@babel/env",{
                "useBuiltIns": "usage"
            }
        ]
    ],
    "plugins": [
        "@babel/plugin-transform-runtime",
        ["import", {
            "libraryName": "antd",
            "libraryDirectory": "es",
            "style": "css"
        }],
        ["@babel/plugin-proposal-decorators", {
            "legacy": true
        }],
        ["@babel/plugin-proposal-class-properties",{
            "loose": true
        }]
    ]
}
